#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/jessekostoss/ontologies/2022/3/Desserts#hasCalories
:hasCalories rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/jessekostoss/ontologies/2022/3/Desserts#hasFlavor
:hasFlavor rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasIngredient ;
           rdfs:domain :Desserts ;
           rdfs:range :Flavor .


###  http://www.semanticweb.org/jessekostoss/ontologies/2022/3/Desserts#hasIngredient
:hasIngredient rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/jessekostoss/ontologies/2022/3/Desserts#hasTexture
:hasTexture rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasIngredient ;
            rdfs:domain :Desserts ;
            rdfs:range :Texture .


###  http://www.semanticweb.org/jessekostoss/ontologies/2022/3/Desserts#hasTopping
:hasTopping rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasIngredient ;
            rdf:type owl:TransitiveProperty ;
            rdfs:domain :Desserts ;
            rdfs:range :Toppings .


#################################################################
